        	System.out.println("Voici la taille de ma branche right" + heightBranch + "\n");
        	        	
        	TableFMatAugmented[rBranchNumber].goToBotTopRetic(u, v, gamma, heightBranch);   
        	System.out.println(" Bot -> Top sur l arete right " + rBranchNumber);
        	
