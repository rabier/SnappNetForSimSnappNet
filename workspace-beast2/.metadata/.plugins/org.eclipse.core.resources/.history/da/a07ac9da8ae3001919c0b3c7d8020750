				double heightBranch=NetworkLeaves[j].getParentByBranch(NetworkLeaves[j].gammaBranchNumber).getHeight();        		
        		TableFMatAugmented[NetworkLeaves[j].gammaBranchNumber].goToTopLeaf(u, v, gamma, heightBranch);  
        		System.out.println("je viens de faire B-> T de l'arete "+NetworkLeaves[j].gammaBranchNumber+"pr feuille "+NetworkLeaves[j].getLabel());
				
